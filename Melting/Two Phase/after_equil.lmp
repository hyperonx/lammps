variable dt equal 0.001


pair_style eam/alloy
pair_coeff * * Mishin-Ni-Al-2009.eam.alloy Ni Al

# velocity all create 300 546445435 mom yes rot yes dist gaussian
min_style cg
minimize 1e-25 1e-25 100000 500000

variable T equal 1400

# run 0
# neighbor 2 bin

# neigh_modify every 10 check yes 
reset_timestep 0
fix 1 all npt temp $T $T $(dt*100) iso 0 0 $(dt*1000) drag 1.0

thermo 1000
thermo_style custom step temp vol press ke pe density

fix temp all print 100 "$(step) $(temp) $(press) $(vol) $(ke) $(pe) $(density)" file temp.txt screen no title  "step temp press vol ke pe density"

fix out_file_1 all print 1000 "$(step) $(temp) $(press) $(vol) $(ke) $(pe) $(density)" file cooling_$T.txt screen no title  "step temp press vol ke pe density"
dump 3 all custom 1000 output.lammpstrj id type x y z 

run 500000

compute z_chunk all chunk/atom bin/1d z lower 0.7 units box
fix 14 all ave/chunk 500000 1 500000 z_chunk density/number file num_density_$T.txt

run 0

write_data after_cooling_$T.in
write_restart after_cooling_$T.lmp 
